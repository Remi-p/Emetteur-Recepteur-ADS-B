\begin_layout Standard
\begin_inset Formula 
\begin{align}
\left|P(f)\right|^{2} & =\left|\mathcal{F}\left(-0.5\times\Pi_{0.5T_{s}}(t-0.25)+0.5\times\Pi_{0.5T_{s}}(t-0.75)\right)\right|^{2}\nonumber \\
 & =\left|0.5\times\left(-0.5T_{s}\text{sinc}(0.5T_{s}\times f)\times\exp(-i2\pi f\times0.25T_{s})+0.5T_{s}\text{sinc}(0.5T_{s}\times f)\times\exp(-i2\pi f\times0.75T_{s})\right)\right|^{2}\nonumber \\
 & =\left|0.25T_{s}\text{sinc}(0.5T_{s}\times f)\left(\exp(-i2\pi f\times0.75T_{s})-\exp(-i2\pi f\times0.25T_{s})\right)\right|^{2}\nonumber \\
 & =\left|\frac{T_{s}\text{sinc}(0.5T_{s}\times f)}{4}\left(e^{-i2\pi f\times0.5T_{s}}\left(e^{-i2\pi f\times0.25T_{s}}-e^{+i2\pi f\times0.25T_{s}}\right)\right)\right|^{2}\label{eq:P(f) eix - e-ix}\\
 & =\left|\frac{T_{s}\text{sinc}(0.5\times f)}{4}\left(e^{-i\pi fT_{s}}\times\sin(\frac{1}{2}\pi fT_{s})\times2i\right)\right|^{2}\label{eq:P(f) => donne sin}
\end{align}

\end_inset


\end_layout

