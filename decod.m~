function [ output_args ] = decod( trame_uncode, decalage, ga )
%DECOD Passe le signal dans la derniere partie de la chaine de
%      communication
%   trame_uncod : Trame surechantillonnee

yl = abs( trame_uncode );

%% ========================== Recepteur ============================== %

% ----------------------- Filtre de reception ------------------------ %
rl = conv(yl_resynch - decalage, ga);

% ------------------------- Echantillonnage -------------------------- %
% rl_d = downsample(rl, Ts/Te);
rl_d = rl(1:Fse:Ns*Fse);

% -------------------------- Bloc decision --------------------------- %
% ------------------ Bloc association symbole->bits ------------------ %
s = pamdemod(rl_d, M, 0);

end

